SELECT * FROM SPACEX;

--Qu 1
SELECT DISTINCT(LAUNCH_SITE) FROM SPACEX;

--Qu 2 
SELECT * FROM SPACEX WHERE LAUNCH_SITE LIKE 'CCA%' LIMIT 5;

--QU 3
SELECT SUM(PAYLOAD_MASS__KG_) FROM SPACEX
	WHERE CUSTOMER = 'NASA (CRS)';
	
--QU 4
SELECT AVG(PAYLOAD_MASS__KG_) FROM SPACEX
	WHERE BOOSTER_VERSION = 'F9 v1.1';

--QU 5
SELECT MIN(DATE) FROM SPACEX
	WHERE LANDING__OUTCOME = 'Success (ground pad)';

--QU 6
SELECT BOOSTER_VERSION FROM SPACEX
	WHERE LANDING__OUTCOME='Success (drone ship)' 
	AND PAYLOAD_MASS__KG_ BETWEEN 4000 AND 6000;

--QU 7
SELECT LANDING__OUTCOME,COUNT(LANDING__OUTCOME) FROM SPACEX
	GROUP BY LANDING__OUTCOME;

--QU 8
SELECT BOOSTER_VERSION FROM SPACEX 
	WHERE PAYLOAD_MASS__KG_ = (
		SELECT MAX(PAYLOAD_MASS__KG_) FROM SPACEX);
		
--QU 9
SELECT LANDING__OUTCOME,BOOSTER_VERSION,LAUNCH_SITE FROM SPACEX
	WHERE LANDING__OUTCOME LIKE 'Failure%';

--QU 10
SELECT LANDING__OUTCOME,COUNT(LANDING__OUTCOME) count FROM SPACEX
	WHERE DATE BETWEEN '2010-06-04' AND '2017-03-20'
	GROUP BY LANDING__OUTCOME
	ORDER BY count DESC;

